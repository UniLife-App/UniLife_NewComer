    "def getUserTag(distInterestCate, distUserInterest, tag):\n",
    "    listInterests = distInterestCate.keys()\n",
    "    for interest in listInterests:\n",
    "        if pd.isna(distUserInterest[interest]) ==False:\n",
    "            for cate in distInterestCate[interest]:\n",
    "                if tag[cate] <=6:\n",
    "                    tag[cate]+=1\n",
    "    return tag"
